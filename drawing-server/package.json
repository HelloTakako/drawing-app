{
  "name": "drawing-server",
  "version": "1.0.0",
  "description": "Drawing Game with Live Voice Chat",
  "main": "index.js",
  "author": "Eduardo Muramoto and Takako Nakajima",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir dist",
    "start": "node dist/index.js",
    "dev": "nodemon --exec npx babel-node src/index.js",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.js\"",
    "pretest": "eslint --ignore-path .gitignore .",
    "postinstall": "rm -rf dist && yarn run build",
    "lint": "yarn prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
    "release": "release-it patch --no-npm.publish"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "apollo-engine": "^1.1.2",
    "apollo-server-express": "^2.12.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-compose": "^7.14.1",
    "graphql-compose-connection": "^6.0.4",
    "graphql-compose-mongoose": "^7.3.5",
    "graphql-middleware": "^4.0.2",
    "graphql-tools": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.9",
    "mongoose-bcrypt": "^1.6.0",
    "mongoose-timestamp": "^0.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "fs-extra": "^9.0.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4"
  }
}
